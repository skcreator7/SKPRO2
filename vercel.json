{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/search\\.html$",
      "dest": "/search.html"
    },
    {
      "src": "^/search$",
      "dest": "/search.html"
    },
    {
      "src": "^/post\\.html$",
      "dest": "/post.html"
    },
    {
      "src": "^/post$",
      "dest": "/post.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/index.html"
    }
  ]
}
